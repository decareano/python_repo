class Solution:
    def plusOne(self, digits):
        pinga = len(digits)
        for i in range(pinga-1, -1, -1):
            if digits[i] < 9:
                digits[i] += 1
               
                return digits
            digits[i] = 0
        return [1] + digits
           

myVar = Solution()
myVar.plusOne([4,3,2,9])
