def removeDuplicates(nums):
    newResult = 0
    
    
    for i in range(1, len(nums)):
        if nums[i] != nums[i-1]:
            nums[newResult] = nums[i]
            print(newResult)
            newResult += 1
        
           
    return newResult

removeDuplicates([0,0,1,1,1,2,2,3,3,4])

